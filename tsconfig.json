{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "./src/app/*"
      ],
      "@app/services/*": [
        "./src/app/services/*"
      ],
      "@app/shared/*": [
        "./src/app/shared/*"
      ],
      "@app/signalr-core/*": [
        "./src/app/signalr-core/*"
      ],
      "@env/*": [
        "./src/enviroments/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "esnext",
    "lib": [
      "esnext",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "skipLibCheck" : true,
    "resolveJsonModule": true,
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "enableIvy": true
  }
}