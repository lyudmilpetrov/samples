{
  "compileOnSave": true,
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "./src/app/*"
      ],
      "@app/services/*": [
        "./src/app/services/*"
      ],
      "@app/shared/*": [
        "./src/app/shared/*"
      ],
      "@app/signalr-core/*": [
        "./src/app/signalr-core/*"
      ],
      "@env/*": [
        "./src/enviroments/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "lib": [
      "ESNext",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
